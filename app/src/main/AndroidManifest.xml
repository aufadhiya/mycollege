<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xyz.aufa.mycollegeassistant1">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconapp"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ActivityClass.frmLogin"
            android:label="Masuk Akun">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityClass.frmDaftar"
            android:label="Daftar Akun"/>
        <activity
            android:name=".ActivityClass.Dashboard"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActivityClass.menuJadwalKuliah"
            android:label="Jadwal Kuliah"
            />
        <activity android:name=".ActivityClass.menuCatatan"
            android:label="Daftar Catatan"
            />
        <activity android:name=".ActivityClass.menuJadwalLain"
            android:label="Jadwal Lain"/>
        <activity
            android:name=".ActivityClass.menuTugas"
            android:label="Daftar Tugas"/><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
     App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>